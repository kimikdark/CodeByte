{
  "scriptFile" : "../CodeByte-1.0-SNAPSHOT.jar",
  "entryPoint" : "com.function.GetbyId.run",
  "bindings" : [ {
    "type" : "httpTrigger",
    "direction" : "in",
    "name" : "req",
    "methods" : [ "GET" ],
    "authLevel" : "ANONYMOUS"
  }, {
    "type" : "cosmosDB",
    "direction" : "in",
    "name" : "produtoInput",
    "partitionKey" : "{query.id}",
    "databaseName" : "codebytebd",
    "containerName" : "inventoryDB",
    "connection" : "CosmosDBConnection",
    "id" : "{query.id}"
  }, {
    "type" : "http",
    "direction" : "out",
    "name" : "$return"
  } ]
}